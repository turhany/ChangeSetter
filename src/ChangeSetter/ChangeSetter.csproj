<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>disable</Nullable>
	<Authors>Türhan Yıldırım</Authors>
	<PackageIcon>changesetter.png</PackageIcon>
	<Description>Set only chaged field(s) between objects.</Description>
	<PackageProjectUrl>https://github.com/turhany/ChangeSetter</PackageProjectUrl>
	<RepositoryUrl>https://github.com/turhany/ChangeSetter</RepositoryUrl>
	<PackageTags>Change Setter</PackageTags>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageVersion>1.0.0</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\img\changesetter.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
